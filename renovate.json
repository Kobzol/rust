{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "schedule": "at any time",
  "dependencyDashboard": true,
  "branchPrefix": "renovatebot/",
  "enabledManagers": [
    "cargo"
  ],
  "forkProcessing": "enabled",
  "includePaths": [
    "library/**"
  ],
  "rangeStrategy": "in-range-only",
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": "at any time"
  }
}
